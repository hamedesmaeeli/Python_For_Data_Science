      "\n",
      "        function validateTokenDataByServer() {\n",
      "            if (ajax.getStatusCode() == 4) {\n",
      "                if (ajax.getStatus() == 200) {\n",
      "                    var code = ajax.getResponseText();\n",
      "                    if (code == \"false\") {\n",
      "                        showErrorMessage(\"در صورتی که از اتصال توکن خود به سیستم مطمئن هستید با مدیر سیستم تماس بگیرید  \");\n",
      "                        return false;\n",
      "                    }\n",
      "                    if (code == \"true\") {\n",
      "                        return true;\n",
      "                    }\n",
      "                    if (code == ERROR_NO_CERT) {\n",
      "                        showErrorMessage(\"در صورتی که از اتصال توکن خود به سیستم مطمئن هستید با مدیر سیستم تماس بگیرید  \");\n",
      "                        return false;\n",
      "                    }\n",
      "                    if (code == ERROR_IN_CERT) {\n",
      "                        showErrorMessage(\"در صورتی که از اتصال توکن خود به سیستم مطمئن هستید با مدیر سیستم تماس بگیرید  \");\n",
      "                        return false;\n",
      "                    }\n",
      "\n",
      "                    if (code == INVALID_USERID_INTOKEN) {\n",
      "\n",
      "                        showErrorMessage(\"اطلاعات توکن شما برای ورود به سیستم کامل نمی باشد.لطفا با مدیر سیستم تماس بگیرید\");\n",
      "                        return false;\n",
      "                    }\n",
      "\n",
      "                    if (code == INVALID_INPUTUSER_NOT_EQUELS_BYTOEKN) {\n",
      "                        showErrorMessage(\"نام کاربری وارد شده با اطلاعات توکن تطابق ندارد.در وارد نمودن نام کاربری دقت فرمایید\");\n",
      "                        return false;\n",
      "                    }\n",
      "\n",
      "                    if (code == ERROR_SYSTEM) {\n",
      "                        showErrorMessage(\"خطا در انجام عملیات .بامدیر سیستم تماس بگیرید\");\n",
      "                        return false;\n",
      "                    }\n",
      "\n",
      "                    if (code.indexOf(\"pki\") !== -1) {\n",
      "                        var errorMsg = code.substring(3, code.length); // to remove the 'pki' prefix\n",
      "                        if (errorMsg === ERROR_PKI_SETUP) {\n",
      "                            showErrorMessage(\"خطا در ارتباط با مرکز CA\");\n",
      "                            return false;\n",
      "                        } else if (errorMsg === ERROR_PKI_PERIOD) {\n",
      "                            showErrorMessage(\"بازه زمانی گواهینامه توکن معتبر نیست\");\n",
      "                            return false;\n",
      "                        } else if (errorMsg === ERROR_PKI_ISSUER) {\n",
      "                            showErrorMessage(\"گواهینامه توکن توسط مرکز CA سازمان تایید نشده است\");\n",
      "                            return false;\n",
      "                        } else if (errorMsg === ERROR_PKI_OFFLINE) {\n",
      "                            showErrorMessage(\"خطا در اعتبارسنجی گواهینامه توکن\");\n",
      "                            return false;\n",
      "                        } else if (errorMsg === ERROR_PKI_OCSPUNK) {\n",
      "                            showErrorMessage(\"خطا در ارتباط با سرویس OCSP\");\n",
      "                            return false;\n",
      "                        } else if (errorMsg === ERROR_PKI_OCSPREVO) {\n",
      "                            showErrorMessage(\"گواهینامه توکن مورد استفاده باطل شده است\");\n",
      "                            return false;\n",
      "                        } else if (errorMsg === ERROR_PKI_CRLREVO) {\n",
      "                            showErrorMessage(\"گواهینامه توکن مورد استفاده باطل شده است\");\n",
      "                            return false;\n",
      "                        } else if (errorMsg === ERROR_PKI_CRLERR) {\n",
      "                            showErrorMessage(\"خطا در ارتباط با سرویس CRL\");\n",
      "                            return false;\n",
      "                        } else if (errorMsg === ERROR_PKI_MAPPING) {\n",
      "                            showErrorMessage(\"توکن استفاده شده برای ورود با مشخصات کاربر تطابق ندارد\");\n",
      "                            return false;\n",
      "                        } else if (errorMsg === ERROR_PKI_KEYUSAGE) {\n",
      "                            showErrorMessage(\"فیلدهای کاربرد (Key Usage) معتبر نیست\");\n",
      "                            return false;\n",
      "                        } else {\n",
      "                            showErrorMessage(\"ورود با توکن به دلیل بروز خطا انجام نشد\");\n",
      "                            return false;\n",
      "                        }\n",
      "                    }\n",
      "\n",
      "                    showErrorMessage(\"خطا در انجام عملیات .بامدیر سیستم تماس بگیرید\");\n",
      "                    return false;\n",
      "                } else {\n",
